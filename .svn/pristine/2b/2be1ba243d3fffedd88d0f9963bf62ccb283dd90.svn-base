<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<html lang="zh-CN">
<head>
<title>自动跳转中...</title>
<script type="text/javascript">
	function authentication() {
		document.getElementById("auto_login_form").submit();
	}
</script>
</head>
<body style="display: none" onload="authentication()">
	<form id="auto_login_form"
		action="${pageContext.request.contextPath}/login.do">
		<input type="submit" />
	</form>
</body>
</html>
