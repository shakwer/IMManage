.add-t i {
  display: inline-block;
  position: relative;
  top: -2px;
  *top: -7px;
  margin-left: 40px;
  *left: 15px;
  width: 10px;
  height: 6px;
  background: url(../images/sprite.png) -76px -2px;
}



/* CSS Document */
body{background:#fff;}
.content{padding:0;background:#fff;}
.content-c{padding:10px 15px;width:auto;}
.dt{width:auto;}
.input-sty1{border-radius:3px;padding:0 7px;line-height:32px;width:105px;color:#666;}
.searchBtn i{background:url(../images/search.png);width:15px;height:15px;vertical-align:-3px;}
.messageBtn i{background:url(../images/message1.png);width:14px;height:11px;vertical-align:-1px;}
.select{width:121px;height:34px;font-size:14px;margin-right:10px;}
.select-t{padding:0 7px;line-height:32px;border-radius:3px;border:solid 1px #dcdcdc;text-align:left;}
.select-t i{float:right;margin:0;top:13px;}
.select-c{border:0;}
.select-c ul{border:solid 1px #dcdcdc;}
.tables1 td.special img{vertical-align:-5px;}
.pageo{width:auto;}
.editBox-text{height:72px;}
.editBox-text span{float:left;width:64px;height:64px;margin-top:4px;border-radius:32px;overflow:hidden;position:relative;}
.editBox-text span img{width:64px;height:64px;}
.editBox-text span em{position:absolute;left:0;top:0;width:64px;height:64px;display:none;background:rgba(0,0,0,0.6) url(../images/photo.png) no-repeat center;border-radius:32px;}
.editBox-text span input{position:absolute;left:0;top:0;width:64px;height:64px;opacity:0;border-radius:32px;}
.editBox-text span:hover em{display:block;}
.editBox-text p{float:right;width:392px;line-height:24px;color:#999;}
.editBox-text p i{float:right;font-size:12px;}
.abs-r{font-size:12px;top:11px;}
.abs-r a{color:#666;}
.abs-r a:hover{color:#df3031;}
.memberNum{float:left;padding:20px 0;font-size:12px;color:#666;line-height:22px;}
.tables1 td{height:auto;padding:13px 10px;font-size:12px;color:#666;line-height:18px;}


.groupList{display:flex;width:100%;font-size:14px;}
.groupList ul{border:solid 1px #dcdcdc;width:50%;height:363px;}
.groupList ul p{height:40px;line-height:40px;background:#f4f4f4;text-align:center;position:relative;}
.groupList ul p label{position:absolute;right:20px;top:0;color:#666;cursor:pointer;}
.groupList ul p input{width:14px;height:14px;vertical-align:-2px;}
.groupList ul p span{position:absolute;right:20px;top:0;background:url(../images/del.png) left center no-repeat;padding-left:20px;color:#666;cursor:pointer;}
.groupList ol{height:323px;overflow:auto;padding:0 20px;}
.groupList ol li{height:50px;padding:15px 0;border-bottom:solid 1px #f0f0f0;line-height:25px;}
.groupList ol li span{float:left;width:50px;height:50px;border-radius:25px;overflow:hidden;}
.groupList ol li span img{width:50px;height:50px;}
.groupList ol li dt{float:left;margin:0 15px;}
.groupList ol li dt font{display:block;}
.groupList ol li dt em{display:block;font-size:12px;color:#999;}
.groupList ol li dd{float:right;width:14px;height:14px;padding:18px 20px;}
.groupList ol li dd input{width:14px;height:14px;cursor:pointer;}
.groupList ol li dd i{display:block;width:14px;height:14px;background:url(../images/del.png);cursor:pointer;}
.groupList dl{width:20px;}
.groupList dl i{float:left;border-left:solid 4px #d6d6d6;border-top:solid 4px #fff;border-bottom:solid 4px #fff;width:0;height:0;margin:177px 0 0 8px;}
.groupNone{margin:70px auto 0 auto;padding-top:170px;background:url(../images/none.png) top center no-repeat;color:#999;text-align:center;}
.messageSend{padding-top:20px;clear:both;}
.messageSend ul{padding-left:10px;border:solid 1px #dcdcdc;}
.messageSend textarea{width:100%;height:100px;border:0;font-size:14px;line-height:20px;padding:5px 0;}
.messageSend button.btn1{border:0;width:180px;height:40px;padding:0;}